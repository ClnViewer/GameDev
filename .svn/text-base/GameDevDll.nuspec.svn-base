<?xml version="1.0"?>
<package>
  <metadata>
    <id>GameDev</id>
    <version>1.0.6.35</version>
    <authors>PS</authors>
    <owners>PS</owners>
    <licenseUrl>https://github.com/ClnViewer/GameDev/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/ClnViewer/GameDev/blob/master/docs/README-WINAPIEXT.md</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Library for interacting with third-party application windows (WINAPI). It is a dialogue device based on scanning and analyzing the contents of windows of a third-party application.</description>
    <releaseNotes>API for using the library in various languages C, C++, C#</releaseNotes>
    <copyright>Copyright PS 2018</copyright>
    <tags>game library winapi scan capture autoclick click native</tags>
    <contentFiles>
      <files include="API/*.cs.pp" buildAction="Content" />
    </contentFiles>
  </metadata>
  <files>
    <file src="README.md"  target="docs"/>
    <file src="API\GameDev.cs.pp" target="content"/>
    <file src="API\*.h" target="content"/>
    <file src="API\*.hpp" target="content"/>
    <file src="API\GameDev.target" target="build\native"/>
    <file src="bin\Debug\GameDev.dll" target="lib\native\32\Debug"/>
    <file src="bin\Debug\libGameDev.a" target="lib\native\32\Debug"/>
    <file src="bin\Debug\libGameDev.def" target="lib\native\32\Debug"/>
    <file src="bin\Debug\GameDevDllTestC++.exe" target="tools\32\Debug"/>
    <file src="bin\Release\GameDev.dll" target="lib\native\32\Release"/>
    <file src="bin\Release\libGameDev.a" target="lib\native\32\Release"/>
    <file src="bin\Release\libGameDev.def" target="lib\native\32\Release"/>
    <file src="bin\Release\GameDevDllTestC++.exe" target="tools\32\Release"/>
  </files>
</package>
